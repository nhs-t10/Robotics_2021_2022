#init:
    goto FirstAuto

#FirstAuto:
    driveRaw (0.5, 0.5, 0.5, 0.5), after 2s next;
    driveRaw (-1, -1, -1, -1), after 1s next;
    driveRaw (1, 1, 1, 1), after 3s next;
    driveRaw (-1, 1, 1, -1), after 3s next;
    driveRaw (0.25, 0.25, 0.25, 0.25), after 4s goto Parking

#Parking:
    driveRaw (0,0,0,0)
