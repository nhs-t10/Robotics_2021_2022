#init:
    goto sampling

#sampling:
    let value = computerVision(), next;
    driveRaw(1, 1, 1, 1), after 10cm driveRaw(0,0,0,0), next;
    if(value == 0) goto firstPosition, if(value == 1) goto secondPosition, if(value == 2) goto Position3

#firstPosition:
    setMotorPower("deposit", 1), after 10cm setMotorPower("deposit", 0), next;
    depositFrieght(), after 3s next;
    goto finalMovement

#secondPosition:
    setMotorPower("deposit", 1), after 20cm setMotorPower("deposit", 0), next;
    depositFrieght(), after 3s next;
    goto finalMovement

#Position3:
    setMotorPower("deposit", 1), after 30cm setMotorPower("deposit", 0), next;
    depositFrieght(), after 3s next;
    goto finalMovement

#finalMovement:
    driveRaw(-1, -1, -1, -1), after 10cm next;
    driveRaw(-1, 1, -1, 1), after 30cm next;
    spinCarousell(), after 3s stopCarousell(), next;
    driveRaw(1, -1, 1, -1), after 30cm driveRaw(0, 0, 0, 0), goto parking

#parking:
    driveRaw(0,0,0,0), stopAllMotors()