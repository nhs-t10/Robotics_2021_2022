#duckTime:
    driveOmni(0,-1,0), after 3meters stopDrive(), next;
    spinner(), after 3s stopSpin(), next;
    goto placeIt

#placeIt:
    driveOmni(0,1,0), after 1.5meters driveOmni(1,0,0), after 1meters next;
    depositFrieght(), after 3s next;
    driveOmni(0,1,0), after 1meters driveOmni(1,0,0), after 0.5meters driveOmni(0,1,0), after 5meters next;
    goto parking

#parking:
    driveRaw(0,0,0,0), stopAllMotors()