#init:
    goto sampling

#sampling:
    let value = computerVision(), next;
    driveRaw(1, 1, 1, 1), after 10cm stopDrive(), next;
    if(value == 0) goto firstPosition, if(value == 1) goto secondPosition, if(value == 2) goto thirdPosition

#firstPosition:
    setMotorPower("deposit", 1), after 10cm setMotorPower("deposit", 0), next;
    depositFrieght(), after 3s next;
    goto duckTime

#secondPosition:
    setMotorPower("deposit", 1), after 20cm setMotorPower("deposit", 0), next;
    depositFrieght(), after 3s next;
    goto duckTime

#thirdPosition:
    setMotorPower("deposit", 1), after 30cm setMotorPower("deposit", 0), next;
    depositFrieght(), after 3s next;
    goto duckTime

